{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "install",
			"args": ["--path", "${workspaceFolder}/.", "--root", "target", "--locked", "--profile", "dev"],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo install"
		}
	]
}